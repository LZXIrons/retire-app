.list-item{
  height: 200rpx;
  display: flex;
  justify-content: center;
  flex-direction: column;
  line-height: 48rpx;
}
.list-item-wrap{
  display: flex;
  justify-content: space-between;
  align-items: center;
  background: #ffffff;
  margin-bottom: 20rpx;
  padding: 0 20rpx;
}
.fixed-bg-color{
  position: fixed;
  width: 100%;
  height: 100%;
  background: #ffffff;
  top: 0;
  left: 0;
  z-index: -1;

}
page{
  position: relative;
}